{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "ApiError.json",
    "type": "object",
    "properties": {
        "code": {
            "type": "string",
            "description": "A machine readable error code"
        },
        "message": {
            "type": "string",
            "description": "A human readable message"
        }
    },
    "required": [
        "code",
        "message"
    ]
}